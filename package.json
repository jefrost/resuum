{
    "name": "resuum",
    "version": "1.0.0",
    "description": "Browser-based resume optimization tool with AI-powered recommendations",
    "type": "module",
    "engines": {
      "node": ">=18.0.0"
    },
    "scripts": {
      "build": "node build.js",
      "dev": "node build.js --watch",
      "test": "vitest",
      "test:ui": "vitest --ui",
      "test:coverage": "vitest --coverage",
      "type-check": "tsc --noEmit",
      "lint": "eslint src --ext .ts,.tsx",
      "preview": "python -m http.server 8080 -d docs",
      "clean": "rm -rf docs"
    },
    "devDependencies": {
      "@types/node": "^20.14.0",
      "@typescript-eslint/eslint-plugin": "^8.0.0",
      "@typescript-eslint/parser": "^8.0.0",
      "@vitest/coverage-v8": "^2.0.0",
      "@vitest/ui": "^2.0.0",
      "esbuild": "^0.21.0",
      "eslint": "^9.7.0",
      "gzip-size": "^7.0.0",
      "jsdom": "^24.1.0",
      "typescript": "^5.5.0",
      "vitest": "^2.0.0"
    },
    "keywords": [
      "resume",
      "optimization",
      "ai",
      "job-search",
      "browser",
      "local-first"
    ],
    "author": "",
    "license": "MIT",
    "repository": {
      "type": "git",
      "url": ""
    }
  }